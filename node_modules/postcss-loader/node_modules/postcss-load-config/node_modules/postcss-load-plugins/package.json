{
  "name": "postcss-load-plugins",
  "version": "2.1.0",
  "description": "Autoload Plugins for PostCSS",
  "engines": {
    "node": ">=0.12",
    "npm": ">=3"
  },
  "main": "index.js",
  "scripts": {
    "lint": "standard",
    "test": "nyc ava test/err/index.js test/rc/index.js test/pkg/index.js test/js/**/index.js",
    "logs": "standard-changelog > CHANGELOG.md",
    "docs": "jsdoc2md index.js lib/plugins.js > INDEX.md",
    "clean": "rm -rf .nyc_output coverage jsdoc-api dmd",
    "start": "sudo npm run clean && npm run lint && sudo npm test"
  },
  "dependencies": {
    "cosmiconfig": "^2.1.1",
    "object-assign": "^4.1.0"
  },
  "devDependencies": {
    "ava": "^0.17.0",
    "coveralls": "^2.11.15",
    "cssnano": "^3.8.1",
    "jsdoc-to-markdown": "^2.0.0",
    "nyc": "^9.0.1",
    "peter": "^1.0.2",
    "postcss": "^5.2.6",
    "postcss-cssnext": "^2.8.0",
    "postcss-import": "^9.0.0",
    "postcss-nested": "^1.0.0",
    "postcss-sprites": "^4.0.0",
    "standard": "^8.6.0",
    "standard-changelog": "0.0.1",
    "sugarss": "^0.2.0"
  },
  "ava": {
    "verbose": "true"
  },
  "nyc": {
    "all": true,
    "include": [
      "index.js",
      "lib"
    ],
    "extension": [
      ".js"
    ]
  },
  "files": [
    "lib",
    "index.js"
  ],
  "keywords": [
    "postcss",
    "postcss-plugin"
  ],
  "author": {
